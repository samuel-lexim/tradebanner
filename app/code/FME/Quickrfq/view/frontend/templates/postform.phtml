                    $('.height').addClass('hidden');
                }
            });
            $("#delivery").change(function () {
                let id = $(this).val();
                if (id === "Pick-Up") {
                    $('.delivery_options').removeClass('active');
                    $('.delivery_options').addClass('hidden');
                } else {
                    $('.delivery_options').removeClass('hidden');
                    $('.delivery_options').addClass('active');
                }
            });
        })
    })
</script>