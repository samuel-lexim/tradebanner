            let discount = 0;

            if (sizes && discounts && sizes.length > 0 && discounts.length > 0) {
                let l_sizes = sizes.length;
                let l_discounts = discounts.length;

                for (i = 0; i < l_sizes; i++) {
                    if (i < (l_sizes - 1) && parseInt(sizes[i]) <= area && area < parseInt(sizes[i + 1])) {
                        if (typeof discounts[i] !== 'undefined') {
                            discount = discounts[i];
                            break;
                        }
                    } else if (i === (l_sizes - 1) && parseInt(sizes[i]) <= area) {
                        if (typeof discounts[i] !== 'undefined') {
                            discount = discounts[i];
                            break;
                        }
                    }
